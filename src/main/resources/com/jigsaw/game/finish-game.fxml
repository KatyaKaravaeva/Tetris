<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPaneMain" prefHeight="400.0" prefWidth="500.0" style="-fx-background-color: #D8BFD8;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jigsaw.game.FinishController">
   <children>
      <AnchorPane fx:id="anchorPaneSmall" layoutY="107.0" prefHeight="293.0" prefWidth="500.0" style="-fx-background-color: #F0FFF0;">
         <children>
            <Label fx:id="info" layoutX="173.0" layoutY="14.0" prefHeight="96.0" prefWidth="198.0" text="Label" textFill="#d22626">
               <font>
                  <Font name="Bernard MT Condensed" size="41.0" />
               </font>
            </Label>
            <Label fx:id="winText" layoutX="23.0" layoutY="143.0" prefHeight="136.0" prefWidth="453.0" text="Label">
               <font>
                  <Font name="Bernard MT Condensed" size="23.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button layoutX="285.0" layoutY="14.0" mnemonicParsing="false" onAction="#startNewGame" prefHeight="78.0" prefWidth="192.0" style="-fx-background-color: #F0FFF0;" text="Start a new game">
         <font>
            <Font name="Bernard MT Condensed" size="18.0" />
         </font>
      </Button>
      <Button layoutX="47.0" layoutY="14.0" mnemonicParsing="false" onAction="#exitFinish" prefHeight="78.0" prefWidth="192.0" style="-fx-background-color: #F0FFF0;" text="Exit">
         <font>
            <Font name="Bernard MT Condensed" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
